{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./",
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "module": "commonjs",
    "removeComments": true,
    "strict": true,
    "target": "es6",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "noUnusedParameters": true
  }
}
